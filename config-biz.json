            },
            "quality":{
               "type":"TITLE",
               "address":"div.single-movie > a > div > div.quality-poster"
            }
         },
         "movieDetails":{
            "banner":{
               "type":"ATTRIB",
               "attrib":"src",
               "address":"div.mvi-content img.hidden"
            },
            "title":{
               "type":"TITLE",
               "address":"div.title > h2"
            },
            "categories":{
