            },
            "description":{
               "type":"TITLE",
               "address":"div.syn-wrapper > p"
            },
            "link":{
               "type":"ATTRIB",
               "attrib":"src",
               "address":"div#plx > p > iframe"
            },
            "servers":{
               "type":"ATTRIB",
               "attrib":"href",
               "address":"div.tab-server > a"
            }
         },
         "seriesDetails":{
            "banner":{
               "type":"ATTRIB",
               "attrib":"src",
               "address":"div.mvi-content img.hidden"
            },
            "title":{
               "type":"TITLE",
